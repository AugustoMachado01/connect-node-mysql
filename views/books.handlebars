<h1>livros disponiveis</h1>
<div class="books-container">
  <a href="/">Adicionar livros</a>
  {{#each books}}
    <div>
      #{{this.id}}
      -
      <a href="books/{{this.id}}"> <span class="bold">{{this.title}}</span></a>
      -
      <span>{{this.pageqty}} paginas</span>
    </div>
  {{/each}}
</div>